
CREATE TABLE NAVIGATOR (
	ID NUMBER NOT NULL,
	PARENT VARCHAR2(20),
	NAVIGATOR_ID VARCHAR2(20) NOT NULL,
	NAVIGATOR_NAME VARCHAR2(20) NOT NULL,
	URL VARCHAR2(50) NOT NULL,
	DESKTOP CHAR(1) NOT NULL,
	MOBILE CHAR(1) NOT NULL,
	DESCRIPTION VARCHAR2(50),
	NAVIGATOR_SEQ NUMBER NOT NULL,
	CREATE_ID VARCHAR2(20) NOT NULL,
	CREATE_DT DATE NULL,
	UPDATE_ID VARCHAR2(20) NOT NULL,
	UPDATE_DT DATE NULL,
	CLASS_TYPE VARCHAR2(100)
);